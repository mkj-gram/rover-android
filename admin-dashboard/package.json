{
    "name": "admin-dashboard",
    "version": "1.0.0",
    "description": "Rover Administration Dashboard",
    "author": "Rover.io",
    "main": "src/index.tsx",
    "scripts": {
        "start": "webpack-dev-server -d --hot --inline --host 0.0.0.0 --port 9000",
        "build": "rm -rf ./dist && webpack --config webpack.config.js",
        "test": "jest"
    },
    "dependencies": {
        "apollo-link": "1.2.2",
        "apollo-link-http": "1.5.4",
        "firebase": "5.0.1",
        "firebaseui": "3.0.0",
        "graphql": "0.13.2",
        "graphql-tag": "2.9.2",
        "react": "16.4.1",
        "react-dom": "16.4.1",
        "react-router-dom": "4.3.1",
        "react-redux": "5.0.7",
        "redux": "4.0.0",
        "redux-thunk": "2.3.0"
    },
    "devDependencies": {
        "@types/react": "16.4.1",
        "@types/react-dom": "16.0.6",
        "@types/react-router-dom": "4.2.7",
        "@types/react-redux": "6.0.3",
        "@types/redux-logger": "3.0.6",
        "@types/redux-thunk": "2.1.0",
        "css-loader": "0.28.11",
        "html-webpack-plugin": "3.2.0",
        "ip": "1.1.5",
        "style-loader": "0.21.0",
        "ts-loader": "4.4.1",
        "tslint": "5.10.0",
        "tslint-loader": "3.6.0",
        "tslint-react": "3.6.0",
        "typescript": "2.8.1",
        "redux-logger": "3.0.6",
        "uglifyjs-webpack-plugin": "1.2.6",
        "webpack": "4.12.0",
        "webpack-cli": "3.0.8",
        "webpack-dev-server": "3.1.4",
        "@types/jest": "23.1.1",
        "jest": "23.1.0",
        "ts-jest": "22.4.6"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js",
            "tsx",
            "jsx"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testPathIgnorePatterns": [
            "/node_modules/",
            "./dist"
        ],
        "coverageReporters": [
            "lcov",
            "html"
        ]
    }
}
