{
  "name": "@rover/audience",
  "version": "1.0.0",
  "description": "Rover Audience Segmentation App",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server -d --hot --inline --port 9001",
    "build": "rm -rf ./dist && webpack --config webpack.config.js",
    "relay": "relay-compiler --src ./src --schema ./src/relay/schema.graphql --watch",
    "schema": "bash runSchema.sh",
    "deploy_build": "NODE_ENV=$NODE_ENV npm run schema && NODE_ENV=$NODE_ENV npm run build"
  },
  "author": "Rover.io",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-relay": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "get-graphql-schema": "^2.1.1",
    "relay-compiler": "^1.1.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@rover/react-bootstrap": "^1.0.0",
    "@rover/react-icons": "^1.1.0",
    "copy-webpack-plugin": "3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "moment": "^2.18.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-data-grid": "^2.0.57",
    "react-data-grid-addons": "^2.0.57",
    "react-dom": "^15.6.1",
    "react-google-maps": "^7.2.0",
    "react-rangeslider": "^2.1.0",
    "react-relay": "^1.1.0"
  }
}
