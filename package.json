    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "get-graphql-schema": "^2.0.0",
    "relay-compiler": "^1.1.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "@rover/react-bootstrap": "^1.0.0",
